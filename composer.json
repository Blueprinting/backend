{
    "name": "blueprinting/backend",
    "type": "library",
    "description": "Framework agnostic view scaffolding for CRUD administrative interfaces.",
    "keywords": ["php", "crud", "scaffolding", "laravel", "symfony"],
    "license": "MIT",
    "prefer-stable": true,
    "version": "0.0.1",
    "scripts": {
        "phpcs": "phpcs --standard=PSR12 ./src ./tests"
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Blueprinting\\": "src/",
            "Blueprinting\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "nyholm/psr7": "^1.3.2",
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12.63"
    }
}
