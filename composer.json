{
    "name": "blueprinting/backend",
    "type": "library",
    "description": "View scaffolding for CRUD administrative tools using Laravel",
    "keywords": [

    ],
    "license": "MIT",
    "prefer-stable": true,
    "scripts": {
        "phpcs": "phpcs --standard=PSR12 ./src ./tests"
    },
    "require": {
        "php": "^7.4",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Blueprinting\\": "src/",
            "Blueprinting\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blueprinting\\Providers\\BlueprintServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "squizlabs/php_codesniffer": "3.*"
    }
}
